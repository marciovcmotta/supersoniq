{
  "name": "supersoniq",
  "version": "0.0.1",
  "description": "SupersoniQ is a lightweight and high-performance messaging broker built with TypeScript and Fastify.",
  "main": "server.ts",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "dist": "tsup src",
    "start:dev": "tsx src/server.ts",
    "start:watch": "tsx watch src/server.ts",
    "start:dist": "yarn dist && node dist/src/app.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marciovcmotta/supersoniq.git"
  },
  "keywords": [
    "nodejs",
    "lightweight",
    "typescript",
    "queue",
    "nosql",
    "websocket",
    "http2",
    "leveldb",
    "message-queue",
    "in-memory",
    "fastify",
    "message-broker"
  ],
  "author": "Marcio Motta",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/marciovcmotta/supersoniq/issues"
  },
  "homepage": "https://github.com/marciovcmotta/supersoniq#readme",
  "devDependencies": {
    "@types/node": "^22.10.1",
    "tsup": "^8.3.5",
    "tsx": "^4.19.2",
    "typescript": "5.7.2"
  },
  "dependencies": {
    "@fastify/cors": "^10.0.1",
    "fastify": "^5.1.0",
    "fastify-websocket": "^4.3.0",
    "leveldown": "^6.1.1",
    "levelup": "^5.1.1",
    "uuid": "^11.0.3"
  }
}
